{
  "extends": [
    "react-app",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["react", "prettier", "import"],
  "rules": {
    "prettier/prettier": "error",
    "react/prop-types": "off",
    "import/order": ["error", { "groups": ["builtin", "external", "internal"] }]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"],
        "extensions": [".js", ".jsx"]
      }
    }
  }
}
